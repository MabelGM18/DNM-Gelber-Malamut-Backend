{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug TS Node",
        "runtimeArgs": ["-r", "ts-node/register"],
        "args": ["${workspaceFolder}/src/index.ts"],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "skipFiles": ["<node_internals>/**"],
        "env": {
          "NODE_ENV": "development"
        }
      }
    ]
  }
  